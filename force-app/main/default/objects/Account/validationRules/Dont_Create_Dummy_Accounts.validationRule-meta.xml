<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dont_Create_Dummy_Accounts</fullName>
    <active>true</active>
    <description>No test or dummy accounts should be added to the system.</description>
    <errorConditionFormula>CONTAINS(Name, "Dummy")</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Do not add dummy accounts.</errorMessage>
</ValidationRule>
